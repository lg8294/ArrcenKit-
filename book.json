
{
  "gitbook": ">3.0.0",
  "author": "lg",
  "language": "zh-hans",
  "root": ".",
  "plugins": [
    "edit-link", 
    "-fontsettings", 
    
    "-lunr", "-search", "search-plus",
    
    "prism",
    "-highlight",
  ],
  "pluginsConfig": {
    "edit-link": {
