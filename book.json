
{
  "gitbook": ">3.0.0",
  "author": "lg",
  "language": "zh-hans",
  "root": ".",
  "plugins": [
    "-fontsettings",
    "-sharing",
    "splitter",
    "-lunr", "-search", "search-plus",
    "-highlight", "prism", "prism-themes",
    "tbfed-pagefooter",
    "sectionx"
  ],
  "pluginsConfig": {
    
    "prism": {
        "css": [
            "prism-themes/themes/prism-xonokai.css"
        ]
    },
    "tbfed-pagefooter": {
        "copyright":"Copyright &copy www.arrcen.com 2017",
        "modify_label": "该文件修订时间：",
        "modify_format": "YYYY-MM-DD HH:mm:ss"
    },
    "sectionx": {
        "tag": "b"
    }
  },
  "links": {
    "sharing": {
      "weibo": true,
      "facebook": false,
      "twitter": false,
      "google": false,
      "instapaper": false,
      "vk": false,
      "all": [
        "weibo"
      ]
    }
  }
}